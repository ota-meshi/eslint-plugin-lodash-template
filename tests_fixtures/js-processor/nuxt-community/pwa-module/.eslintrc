
{
  "root": true,
  "extends": "standard",
  "plugins": [
      "jest"
  ],
  "env": {
      "jest/globals": true
  },
  "overrides": [
    {
      "files": "*.js",
      "extends": [
        "plugin:lodash-template/recommended-with-js"
      ],
      "parserOptions": {
        "parser": "babel-eslint"
      },
      "globals": {
        "options": true,
        "serialize": true
      }
    }
  ]
}