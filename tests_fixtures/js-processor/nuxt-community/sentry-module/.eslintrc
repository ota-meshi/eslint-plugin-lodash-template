{
  "root": true,
  "extends": ["@nuxtjs"],
  "overrides": [
    {
      "files": ["*.js"],
      "extends": ["plugin:lodash-template/base"],
      "processor": "lodash-template/js",
      "parserOptions": {
        "parser": "babel-eslint"
      },
      "globals": {
        "options": true,
        "serialize": true,
        "serializeFunction": true
      }
    }
  ]
}
