{
  "name": "eslint-plugin-lodash-template",
  "version": "0.4.0",
  "description": "ESLint plugin for John Resig-style micro templating(Lodash/Underscore.js template)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test:base": "mocha \"tests/**/*.js\" --reporter dot",
    "test": "nyc --reporter=lcov npm run test:base -- --timeout 60000",
    "test:d": "mocha --inspect --debug-brk \"tests/**/*.js\" --reporter dot",
    "update": "node ./tools/update.js && npm run lint:fix && npm run test",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/eslint-plugin-lodash-template.git"
  },
  "author": "yosuke ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/eslint-plugin-lodash-template/issues"
  },
  "homepage": "https://github.com/ota-meshi/eslint-plugin-lodash-template#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-config",
    "lodash-templates",
    "lodash",
    "underscore-templates",
    "underscore",
    "micro-templates",
    "micro",
    "microtemplate",
    "resig-templates",
    "resig",
    "John Resig micro templates",
    "template",
    "templates",
    "templating",
    "rules"
  ],
  "peerDependencies": {
    "eslint": "^4.15.0 || >=5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "4.15.0",
    "eslint-config-mysticatea": "^13.0.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-eslint-plugin": "^1.4.0",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "espree": "3.5.2",
    "fs-extra": "^5.0.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "parse5": "^4.0.0",
    "esquery": "^1.0.0"
  }
}
