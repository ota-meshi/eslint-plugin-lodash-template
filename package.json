{
  "name": "eslint-plugin-lodash-template",
  "version": "0.12.0",
  "description": "ESLint plugin for John Resig-style micro templating(Lodash/Underscore.js template)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "if-node-version \">=8\" eslint . --ext .js,.vue",
    "pretest": "npm run lint",
    "test:base": "mocha \"tests/**/*.js\" --reporter dot",
    "test": "if-node-version \">=8\" npm run test:nyc && if-node-version \"<=4\" npm run test:v:node4 && if-node-version \">4\" npm run test:v:low && if-node-version \">4\" npm run test:v:node6",
    "test:nyc": "nyc --reporter=lcov npm run test:base -- --timeout 60000",
    "test:d": "mocha --inspect --debug-brk \"tests/**/*.js\" --reporter dot",
    "watch": "npm run test:base -- --watch",
    "update": "node ./tools/update.js && npm run lint:fix && npm run test",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "test:sp": "mocha \"test_sp/**/*.js\" --reporter dot",
    "test:v:node6": "npm run test:v:high && npm run test:v:latest && npm i",
    "test:v:node4": "npm i babel-eslint@8 --no-save && npm run test:v:low && npm i",
    "test:v:high": "npm i eslint@5.0.0 parse5@5.0.0 --no-save && npm run test:base",
    "test:v:low": "npm i eslint@4.15.0 parse5@4.0.0 --no-save && npm run test:base",
    "test:v:latest": "npm i eslint@latest parse5@latest --no-save && npm run test:base",
    "new": "node ./tools/new-rule.js",
    "pg:build": "webpack --config playground/webpack.config.js --progress --mode production",
    "pg:dev": "webpack --config playground/webpack.config.js --progress --mode development"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/eslint-plugin-lodash-template.git"
  },
  "author": "yosuke ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/eslint-plugin-lodash-template/issues"
  },
  "homepage": "https://github.com/ota-meshi/eslint-plugin-lodash-template#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-config",
    "lodash-templates",
    "lodash",
    "underscore-templates",
    "underscore",
    "micro-templates",
    "micro",
    "microtemplate",
    "resig-templates",
    "resig",
    "John Resig micro templates",
    "template",
    "templates",
    "templating",
    "rules"
  ],
  "peerDependencies": {
    "eslint": "^4.15.0 || >=5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@mysticatea/eslint-plugin": "^8.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-es": "^1.4.0",
    "eslint-plugin-local": "^1.0.0",
    "eslint4b": "^5.10.0",
    "if-node-version": "^1.1.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "pako": "^1.0.7",
    "prettier": "^1.15.3",
    "style-loader": "^0.23.1",
    "vue": "^2.5.21",
    "vue-eslint-editor": "^0.1.4",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "esquery": "^1.0.1",
    "parse5": "^4.0.0 || ^5.0.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
