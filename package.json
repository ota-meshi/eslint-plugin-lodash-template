{
  "name": "eslint-plugin-lodash-template",
  "version": "0.13.0",
  "description": "ESLint plugin for John Resig-style micro templating(Lodash/Underscore.js template)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "node -e \"if(process.env.ESLINT=='low'||process.env.ESLINT=='high')process.exit(1)\" && eslint . --ext .js,.vue || node -e \"if(process.env.ESLINT!='low'&&process.env.ESLINT!='high')process.exit(1)\"",
    "pretest": "npm run lint",
    "test:base": "mocha \"tests/**/*.js\" --reporter dot",
    "test": "npm run test:nyc",
    "test:nyc": "nyc --reporter=lcov npm run test:base -- --timeout 60000",
    "test:d": "mocha --inspect --debug-brk \"tests/**/*.js\" --reporter dot",
    "watch": "npm run test:base -- --watch",
    "update": "node ./tools/update.js && npm run lint:fix && npm run test",
    "updateonly": "node ./tools/update.js && npm run lint:fix",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "test:sp": "mocha \"test_sp/**/*.js\" --reporter dot",
    "new": "node ./tools/new-rule.js",
    "copy:images": "cpx \"logo.*\" \"docs/\\.vuepress/public\" -u",
    "docs:watch": "npm run copy:images && vuepress dev docs",
    "docs:build": "npm run copy:images && vuepress build docs",
    "docs:deploysetup": "npm run docs:build && npm run docs:clean && npm run docs:copy",
    "docs:clean": "rimraf assets",
    "docs:copy": "cpx \"docs/\\.vuepress/dist/**\" . -u"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/eslint-plugin-lodash-template.git"
  },
  "author": "yosuke ota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/eslint-plugin-lodash-template/issues"
  },
  "homepage": "https://github.com/ota-meshi/eslint-plugin-lodash-template#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "eslint-config",
    "lodash-templates",
    "lodash",
    "underscore-templates",
    "underscore",
    "micro-templates",
    "micro",
    "microtemplate",
    "resig-templates",
    "resig",
    "John Resig micro templates",
    "template",
    "templates",
    "templating",
    "rules"
  ],
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^12.0.0",
    "babel-eslint": "^10.0.3",
    "cpx": "^1.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-es": "^2.0.0",
    "eslint-plugin-lodash-template": "file:.",
    "eslint4b": "^6.5.1",
    "if-node-version": "^1.1.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "pako": "^1.0.10",
    "prettier": "^1.18.2",
    "semver": "^6.3.0",
    "vue-eslint-editor": "^0.1.4",
    "vuepress": "^1.1.0"
  },
  "dependencies": {
    "esquery": "^1.0.1",
    "parse5": "^5.0.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  }
}
